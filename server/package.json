{
  "name": "habittracker-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node src/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/tests/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "express": "^4.17.1",
    "ramda": "^0.27.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/ramda": "types/npm-ramda#dist",
    "jest": "^25.5.4",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
